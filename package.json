{
  "name": "dnd_api_helper",
  "repository": {
    "type": "git",
    "url": "https://github.com/wachtelhund/dnd_api_helper"
  },
  "keywords": [
    "dungeons and dragons",
    "edward blom",
    "dnd",
    "api",
    "helper"
  ],
  "files": [
    "build"
  ],
  "version": "2.5.6",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run bun || npm run start-fallback",
    "bun": "bun run build && bun build/index.js",
    "start-fallback": "npm run build && node build/index.js",
    "test": "jest",
    "dev": "npx nodemon",
    "build": "rimraf ./build && tsc",
    "lint": "./node_modules/.bin/eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "./node_modules/.bin/prettier --ignore-path jest.config.js .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.7.0",
    "bun-types": "^1.0.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0"
  }
}
